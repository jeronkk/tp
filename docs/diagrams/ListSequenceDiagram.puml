@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":ListCommandParser" as ListCommandParser LOGIC_COLOR
participant "l:ListCommand" as ListCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("list" + PREFIX)
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("list" + PREFIX)
activate AddressBookParser

create ListCommandParser
AddressBookParser -> ListCommandParser
activate ListCommandParser

ListCommandParser --> AddressBookParser
deactivate ListCommandParser

AddressBookParser -> ListCommandParser : parse(PREFIX)
activate ListCommandParser

create ListCommand
ListCommandParser -> ListCommand : new(Predicate, successMessage, keyword)
activate ListCommand

ListCommand --> ListCommandParser
deactivate ListCommand

ListCommandParser --> AddressBookParser : l
deactivate ListCommandParser
destroy ListCommandParser

AddressBookParser --> LogicManager : l
deactivate AddressBookParser

LogicManager -> ListCommand : execute(m)
activate ListCommand

ListCommand -> Model : getFilteredList(predicate)
activate Model
Model --> ListCommand
deactivate Model

create CommandResult
ListCommand -> CommandResult : new(resultMessage)
activate CommandResult

CommandResult --> ListCommand
deactivate CommandResult

ListCommand --> LogicManager : r
deactivate ListCommand

[<-- LogicManager
deactivate LogicManager
@enduml


@startuml
title List Command Variants

actor User

participant "UI" as UI
participant "LogicManager" as LM
participant "Parser" as Parser
participant "ListCommand" as ListCommand
participant "Model" as Model
participant "Result" as Result

User -> UI: Enter list command
UI -> LM: execute(command)
LM -> Parser: parseCommand(command)
alt command == "list"
    Parser -> ListCommand: new(filter = none)
else command == "list t/"
    Parser -> ListCommand: new(filter = "t")
else command == "list tt/"
    Parser -> ListCommand: new(filter = "tt")
end
LM -> ListCommand: execute(Model)
ListCommand -> Model: getList(predicate, successMessage, keyword)
Model --> ListCommand: filteredPersonList
ListCommand -> LM: result
LM -> UI: display result
@enduml
